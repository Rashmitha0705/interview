    public static void main(String args[])
    {
        int arr[]={0,1};
        Arrays.sort(arr);
        int n=arr[arr.length-1];
        int sm=(n*(n+1))/2;
        int curr=0;
        for(int i=0;i<arr.length;i++)
        {
            curr+=arr[i];
        }
        int miss= sm-curr;
        System.out.println(miss);
                
                
                
    }
